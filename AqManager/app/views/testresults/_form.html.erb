<%= form_for [@parameter, @testresult] do |f| %>
    <%= f.error_messages %>
    <p xmlns="http://www.w3.org/1999/html">
      <%= f.label :value %></br>
      <%= f.text_field :value %>
    </p>
    <p>
      <%= f.label :test_date %> <br/>
      <%= datetime_select :testresult, :testresult_date %>
    </p>
    <p>
      <%= f.label :note %> <br />
      <%= f.text_area :note %>
    </p>
    <%=  f.submit %>

<% end %>